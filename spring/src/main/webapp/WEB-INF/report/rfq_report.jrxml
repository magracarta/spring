	<queryString>
		<![CDATA[select
   r.rfq_no as rfq_no,
   r.cust_no,
   (select nvl(ed_misu_amt,0) from t_cust_deal_amt cda where cda.cust_no = r.cust_no and cda.deal_mon =  '202310' /**P*/) as misu_amt,
   to_char(to_date(rfq_dt, 'yyyymmdd'), 'yyyy-mm-dd') as rfq_dt,
   rfq_type_cd,
   to_char(to_date(expire_dt, 'yyyymmdd'), 'yyyy-mm-dd') as expire_dt,
   cust_name,
   case when hp_no is not null then substr(hp_no, 0, 3) || '-' || substr(hp_no, 4, 4) || '-' || substr(hp_no, 8, 4) end as hp_no,
   case when tel_no is not null then substr(tel_no, 0, 3) || '-' || substr(tel_no, 4, 3) || '-' || substr(tel_no, 7, 4) end as tel_no,
   case when fax_no is not null then substr(fax_no, 0, 3) || '-' || substr(fax_no, 4, 3) || '-' || substr(fax_no, 7, 4) end as fax_no,
   email,
   post_no,
   addr1,
   addr2,
   addr1 || addr2 as addr,
   breg_seq,
   breg_name,
   breg_no,
   breg_rep_name,
   rfq_org_code,
   (select org_kor_name from t_org where org_code = r.rfq_org_code) as rfq_org_name,
   rfq_mem_no,
   (select kor_name from t_mem where mem_no = r.rfq_mem_no) as rfq_mem_name,
   office_post_no,
   office_addr1,
   office_addr2,
   office_tel_no,
   office_fax_no,
   memo,
   total_amt,
   total_amt * 0.1 as vat_amt,
   total_amt * 1.1 as rfq_amt,
   '(' || total_amt_str || ' 원정)' as total_amt_str,
   '(' || vat_amt_str || ' 원정)' as vat_amt_str,
   '(' || rfq_amt_str || ' 원정)' as rfq_amt_str,
   discount_rate,
   discount_amt,
   rfq_amt,
   plant.machine_name,
   plant.machine_plant_seq,
   plant.maker_cd,
   (select code_name from t_code where group_code =  'MAKER' /**P*/ and code = plant.maker_cd) as maker_name, 
   plant.machine_type_cd,
   plant.machine_sub_type_cd,
   (select code_name from t_code where group_code =  'MACHINE_TYPE' /**P*/ and code = plant.machine_type_cd) as machine_type_name,
   agency_price,
   write_price,
   review_price,
   agree_price,
   max_dc_price,
   fee_price,
   sale_price,
   part_free_amt,
   part_cost_amt,
   attach_amt,
   show_yn,
   tcmr.c_mch_request_seq,
   tcmr.request_dt,
   to_char(tcmr.reg_date, 'yyyy-mm-dd hh24:mi') as request_date,
   tcmr.request_text,
   r.rfq_send_yn,
   (select sale_area_code from v_cust where cust_no = r.cust_no) as sale_area_code,
   decode(tcmr.c_mch_request_seq, null, decode(r.data_source_cd,  'ERP_WEB' , 'Y', 'N'), 'Y') as send_rfq_btn_show_yn,
	 decode(r.data_source_cd,  'ERP_WEB' , 'ERP', '고객앱') as request_source,
	 yk_breg_no,
	 yk_breg_name,
	 yk_breg_cor_type,
	 yk_breg_cor_part,
	 yk_tel_no,
	 yk_fax_no,
	 yk_addr,
     yk_addr1,
	 yk_addr2,
	 yk_breg_rep_name || '       (인)' as yk_breg_rep_name
from t_rfq r
left outer join t_rfq_machine m on r.rfq_no = m.rfq_no
left outer join t_machine_plant plant on plant.machine_plant_seq = m.machine_plant_seq
left outer join t_c_mch_request tcmr on tcmr.c_mch_request_seq = m.c_mch_request_seq
left outer join (
	select translate 
	     ( substr(total_amt, 1,1)||decode(substr(total_amt, 1,1),0,'','천') 
	    || substr(total_amt, 2,1)||decode(substr(total_amt, 2,1),0,'','백') 
	    || substr(total_amt, 3,1)||decode(substr(total_amt, 3,1),0,'','십') 
	    || substr(total_amt, 4,1)||decode(substr(total_amt, 1,4),0,'','조') 
	    || substr(total_amt, 5,1)||decode(substr(total_amt, 5,1),0,'','천') 
	    || substr(total_amt, 6,1)||decode(substr(total_amt, 6,1),0,'','백') 
	    || substr(total_amt, 7,1)||decode(substr(total_amt, 7,1),0,'','십') 
	    || substr(total_amt, 8,1)||decode(substr(total_amt, 5,4),0,'','억') 
	    || substr(total_amt, 9,1)||decode(substr(total_amt, 9,1),0,'','천') 
	    || substr(total_amt,10,1)||decode(substr(total_amt,10,1),0,'','백') 
	    || substr(total_amt,11,1)||decode(substr(total_amt,11,1),0,'','십') 
	    || substr(total_amt,12,1)||decode(substr(total_amt, 9,4),0,'','만') 
	    || substr(total_amt,13,1)||decode(substr(total_amt,13,1),0,'','천') 
	    || substr(total_amt,14,1)||decode(substr(total_amt,14,1),0,'','백') 
	    || substr(total_amt,15,1)||decode(substr(total_amt,15,1),0,'','십') 
	    || substr(total_amt,16,1) , '1234567890', '일이삼사오육칠팔구') total_amt_str
	    ,translate 
	     ( substr(vat_amt, 1,1)||decode(substr(vat_amt, 1,1),0,'','천') 
	    || substr(vat_amt, 2,1)||decode(substr(vat_amt, 2,1),0,'','백') 
	    || substr(vat_amt, 3,1)||decode(substr(vat_amt, 3,1),0,'','십') 
	    || substr(vat_amt, 4,1)||decode(substr(vat_amt, 1,4),0,'','조') 
	    || substr(vat_amt, 5,1)||decode(substr(vat_amt, 5,1),0,'','천') 
	    || substr(vat_amt, 6,1)||decode(substr(vat_amt, 6,1),0,'','백') 
	    || substr(vat_amt, 7,1)||decode(substr(vat_amt, 7,1),0,'','십') 
	    || substr(vat_amt, 8,1)||decode(substr(vat_amt, 5,4),0,'','억') 
	    || substr(vat_amt, 9,1)||decode(substr(vat_amt, 9,1),0,'','천') 
	    || substr(vat_amt,10,1)||decode(substr(vat_amt,10,1),0,'','백') 
	    || substr(vat_amt,11,1)||decode(substr(vat_amt,11,1),0,'','십') 
	    || substr(vat_amt,12,1)||decode(substr(vat_amt, 9,4),0,'','만') 
	    || substr(vat_amt,13,1)||decode(substr(vat_amt,13,1),0,'','천') 
	    || substr(vat_amt,14,1)||decode(substr(vat_amt,14,1),0,'','백') 
	    || substr(vat_amt,15,1)||decode(substr(vat_amt,15,1),0,'','십') 
	    || substr(vat_amt,16,1) , '1234567890', '일이삼사오육칠팔구') vat_amt_str
	    ,translate 
	     ( substr(rfq_amt, 1,1)||decode(substr(rfq_amt, 1,1),0,'','천') 
	    || substr(rfq_amt, 2,1)||decode(substr(rfq_amt, 2,1),0,'','백') 
	    || substr(rfq_amt, 3,1)||decode(substr(rfq_amt, 3,1),0,'','십') 
	    || substr(rfq_amt, 4,1)||decode(substr(rfq_amt, 1,4),0,'','조') 
	    || substr(rfq_amt, 5,1)||decode(substr(rfq_amt, 5,1),0,'','천') 
	    || substr(rfq_amt, 6,1)||decode(substr(rfq_amt, 6,1),0,'','백') 
	    || substr(rfq_amt, 7,1)||decode(substr(rfq_amt, 7,1),0,'','십') 
	    || substr(rfq_amt, 8,1)||decode(substr(rfq_amt, 5,4),0,'','억') 
	    || substr(rfq_amt, 9,1)||decode(substr(rfq_amt, 9,1),0,'','천') 
	    || substr(rfq_amt,10,1)||decode(substr(rfq_amt,10,1),0,'','백') 
	    || substr(rfq_amt,11,1)||decode(substr(rfq_amt,11,1),0,'','십') 
	    || substr(rfq_amt,12,1)||decode(substr(rfq_amt, 9,4),0,'','만') 
	    || substr(rfq_amt,13,1)||decode(substr(rfq_amt,13,1),0,'','천') 
	    || substr(rfq_amt,14,1)||decode(substr(rfq_amt,14,1),0,'','백') 
	    || substr(rfq_amt,15,1)||decode(substr(rfq_amt,15,1),0,'','십') 
	    || substr(rfq_amt,16,1) , '1234567890', '일이삼사오육칠팔구') rfq_amt_str
	    , rfq_no
	from (select rfq_no
				, lpad(total_amt,16 ,'0') as total_amt
				, lpad(total_amt * 0.1,16 ,'0') as vat_amt
				, lpad(total_amt * 1.1,16 ,'0') as rfq_amt
		  from t_rfq r 
		  where 1=1
	)  
) amt on amt.rfq_no = r.rfq_no
left outer join (
select 
	  a.cust_no           as yk_cust_no      
 	, a.cust_name         as yk_cust_name    
 	, a.tel_no            as yk_tel_no       
 	, a.fax_no            as yk_fax_no       
 	, a.email 			  as yk_email        
 	, a.hp_no 			  as yk_hp_no        
 	, a.post_no           as yk_post_no      
 	, a.addr1 			  as yk_addr1        
 	, a.addr2 			  as yk_addr2        
 	, a.addr1 || ' ' || a.addr2 as yk_addr 
 	, c.breg_name         as yk_breg_name    
 	, substr(c.breg_no, 1, 3) || '-' || substr(c.breg_no, 4, 2) || '-' || substr(c.breg_no, 6, 5)            as yk_breg_no      
 	, c.breg_rep_name     as yk_breg_rep_name
 	, c.breg_cor_type     as yk_breg_cor_type
 	, c.breg_cor_part     as yk_breg_cor_part
 	, c.biz_post_no       as yk_biz_post_no  
 	, c.biz_addr1         as yk_biz_addr1    
 	, c.biz_addr2         as yk_biz_addr2    
 	, c.biz_addr1 || ' ' || c.biz_addr2 as yk_biz_addr  
 from t_cust a 
 inner join t_cust_breg b on a.cust_no  = b.cust_no and b.use_yn = 'Y' 
 inner join v_breg c on b.breg_seq  = c.breg_seq  and c.use_yn  = 'Y' 
 where 1=1 
 and a.cust_no =  '20060727140532287' 
 and c.breg_type_cd  =  'COR' 
) yk on 1=1
where r.rfq_no =  $P{rfq_no}]]>
