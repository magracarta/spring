		<queryString language="SQL">
			<![CDATA[select ra.rfq_no as paid_rfq_no,
		 ra.seq_no as paid_seq_no,
		 ra.machine_basic_part_seq as paid_machine_basic_part_seq,
		 ra.part_no as paid_part_no,
		 part.unit as paid_unit,
		 nvl(ra.part_name, d.part_name) as paid_part_name,
       d.part_name_change_yn as paid_part_name_change_yn,
		 ra.free_yn as paid_free_yn,
		 ra.default_qty as paid_default_qty,
		 ra.add_qty as paid_add_qty,
		 ra.unit_price as paid_unit_price,
       ra.total_amt as paid_total_amt,
       rownum
