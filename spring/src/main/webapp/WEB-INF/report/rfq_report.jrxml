left outer join t_machine_basic_part part on ra.machine_basic_part_seq = part.machine_basic_part_seq
left outer join t_part d on ra.part_no = d.part_no 
where ra.use_yn = 'Y' 
and ra.free_yn <> 'Y'
and ra.rfq_no =  $P{rfq_no}]]>
