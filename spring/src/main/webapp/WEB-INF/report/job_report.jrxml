	<queryString>
		<![CDATA[select
	a.job_report_no as job_report_no -- 접수번호
	, substr(a.in_dt, 0, 4) || '-' || substr(a.in_dt, 5, 2) || '-' || substr(a.in_dt, 7, 2) as in_dt -- 입고일자
	, (select max(inout_doc_no) from t_inout_doc where job_report_no = a.job_report_no and inout_doc_type_cd = '07' and inout_type_cd = '04' and use_yn = 'Y') as inout_doc_no -- 전표번호
	, b.machine_name as machine_name -- 장비명
    , substr(b.out_dt, 0, 4) || '-' || substr(b.out_dt, 5, 2) || '-' || substr(b.out_dt, 7, 2) as out_dt -- 출하일자
    , b.body_no as body_no -- 차대번호
    , b.engine_no_1 as engine_no_1 -- 엔진번호1
    , (select max(op_hour) from t_machine_op_time where machine_seq = a.machine_seq and op_time_type_cd = 'JOB_REPORT' and op_dt = a.consult_dt) as op_hour -- 가동시간
	, d.cust_name as cust_name -- 차주명
	, substr(d.hp_no, 0, 3) || '-' || substr(d.hp_no, 4, 4) || '-' || substr(d.hp_no, 8, 4) as hp_no -- 휴대폰
	, d.addr1 as addr1 -- 주소1
	, d.addr2 as addr2 -- 주소2
	, case when (a.receipt_type_rt = 'R') then '사전'
	       when (a.receipt_type_rt = 'T') then '당일'
	       when (a.receipt_type_rt = 'A') then 'APP'
	  end as receipt_type -- 접수구분
	, a.travel_area_name as travel_area_name -- 출장지역
	-- , (select code_name from t_code where group_code = 'JOB_TYPE' and code = a.job_type_cd) as job_type_name
	, case when (a.job_type2_cd is null)
		     then (select code_name from t_code where group_code = 'JOB_TYPE' and code = a.job_type_cd)
		     else (select code_name from t_code where group_code = 'JOB_TYPE' and code = a.job_type_cd) || ' | ' || (select code_name from t_code where group_code = 'JOB_TYPE2' and code = a.job_type2_cd)
	  end as job_type_name
	, case when (a.job_case_ti = 'T') then '출장'
	       when (a.job_case_ti = 'I') then '입고'
	  end as job_case -- 정비종류
    -- 수리비용, 부품비용은 합계로
	, a.plan_travel_expense as plan_travel_expense -- 예상출장비
	, a.travel_expense as travel_expense -- 발생출장비
	, (a.total_amt * 1.1) as total_vat_amt -- 비용합계(VAT포함)
    , g.org_kor_name -- 센터명
	, (select kor_name from t_mem where mem_no = a.reg_id) as reg_mem_name -- 담당자
	, p.total_plan_repair -- 예상수리비용
	, p.total_repair -- 발생수리비용
	, pp.plan_part_amt -- 예상부품비용
	, pp.part_amt -- 발생부품비용
	, a.job_confirm_date -- 확인일시
	, (select kor_name from t_mem where mem_no = a.job_mem_no) as job_mem_name -- 작업직원
from t_job_report a
inner join v_machine b on b.machine_seq = a.machine_seq and b.use_yn <> 'N'
inner join v_cust d on d.cust_no = a.cust_no
left outer join t_org g on g.org_code = a.org_code and g.use_yn <> 'N'
left outer join (
	select
		sum(plan_work_amt) as total_plan_repair -- 예상수리비용
		, sum(work_amt) as total_repair -- 발생수리비용
		, job_report_no
	from (
		select
		    job_report_order_seq
		    , plan_work_amt -- 예상공임
		    , work_amt -- 발생비용
		    , job_report_no
		from t_job_report_order
		where 1=1
			and job_report_no = $P{job_report_no}
			and use_yn <> 'N'
	) where job_report_order_seq > 0
	group by job_report_no
) p on p.job_report_no = a.job_report_no
left outer join (
	select
        a.job_report_no
        , sum(nvl(a.qty, 1) * a.unit_price) as plan_part_amt -- 예상 부품비용
        , sum(nvl(a.use_qty, 1) * a.unit_price) as part_amt -- 발생 부품비용 (청구금액)
    from t_job_report_part a
    where 1=1
        and a.job_report_no = $P{job_report_no}
        and a.use_yn <> 'N'
    group by job_report_no
) pp on pp.job_report_no = a.job_report_no
where 1=1
    and a.job_report_no = $P{job_report_no}]]>
	</queryString>
