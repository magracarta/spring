		<parameter name="job_report_no" class="java.lang.String">
			<defaultValueExpression><![CDATA["JR20150204-029"]]></defaultValueExpression>
		</parameter>
		<queryString language="SQL">
			<![CDATA[select
    a.*
    , row_number () over(order by a.storage_name asc) as rnk
from (
select
	a.seq_no as seq_no -- 순번
	, a.part_no as part_no -- 부품번호
	, nvl(a.part_name, fnc_part_name(a.part_no)) as part_name -- 부품명
	, a.qty as qty -- 수량
	, a.unit_price as unit_price -- 단가
	, (nvl(a.qty, 1) * a.unit_price) as amount -- 금액
	, (
		select storage_name from t_part_stock_stat
		where 1=1
		and part_no = a.part_no
		and stock_year = substr(c.consult_dt, 1, 4)
		and warehouse_cd = case when (c.org_code =  '5110') then  '6000' else c.org_code end
	) as storage_name
    from t_job_report_part a
	left outer join (
		select
			job_report_no
			, job_seq_no
			, part_no
			, doc_barcode_no -- 전표바코드
		from t_part_inout
		where use_yn <> 'N' and job_report_no =  $P{job_report_no}
		group by job_report_no, part_no, job_seq_no
			, doc_barcode_no
	) b on b.job_report_no = a.job_report_no and b.part_no = a.part_no and b.job_seq_no = a.seq_no
    left join t_job_report c on a.job_report_no = c.job_report_no
    where 1=1
        and a.job_report_no = $P{job_report_no}
        and a.use_yn <> 'N'
    order by a.seq_no
) a
order by seq_no]]>
		</queryString>
